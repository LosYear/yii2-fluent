{
    "name": "losyear/yii2-fluent",
    "description": "Yii2 Fluent CMS core module",
    "type": "yii2-extension",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Losev Yaroslav",
            "email": "losevyaroslav@gmail.com"
        }
    ],

    "repositories" : [
        {
            "type": "vcs",
            "url": "https://github.com/iutbay/yii2-kcfinder"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "rmrevin/yii2-fontawesome": "~2.12",
        "2amigos/yii2-ckeditor-widget": "~1.0",
        "iutbay/yii2-kcfinder": "*",
        "himiklab/yii2-easy-thumbnail-image-helper": "*",
		"codemix/yii2-localeurls": "^1.4"
    },

    "autoload": {
        "psr-4": {
            "yii\\fluent\\": ""
        }
    },

    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
